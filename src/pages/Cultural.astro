---
//Pagina de Noticias Categoria Cultural
import Layout from '../layouts/Layout.astro';
import Header from '../components/Header.astro';
import BannerExtra from '../components/BannerExtra.astro';
import Footer from '../components/Footer.astro';
import SectionCultural from '../components/SectionCultural.astro';
//Se importa la Plantilla a usar y los componentes que conformaran la pagina
// los guiones bajo (-) dividen el apartado de astro para los import
---
<!-- Se hace uso de los elementos-->
<!-- Plantilla que recibe un valor tipo texto porque asi fue declarado en tu archivo-->
<Layout title="Culturales">
	<Header/>
	<BannerExtra title="Culturales" descripcion="Todas las noticias del ambito cultural"/>
	<!-- bannerExtra recibe un titulo y descripcion tipo texto-->
	<SectionCultural/>
	<Footer/>
</Layout>
